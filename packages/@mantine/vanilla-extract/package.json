{
  "name": "@mantine/vanilla-extract",
  "version": "8.2.1",
  "description": "Vanilla Extract integration for Mantine theme",
  "homepage": "https://mantine.dev",
  "license": "MIT",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "keywords": [
    "frontend",
    "library",
    "next",
    "nextjs",
    "react",
    "vanilla-extract"
  ],
  "sideEffects": false,
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
