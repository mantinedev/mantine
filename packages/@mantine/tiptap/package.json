{
  "name": "@mantine/tiptap",
  "version": "7.3.0-alpha.0",
  "description": "Rich text editor based on tiptap",
  "homepage": "https://mantine.dev/others/tiptap",
  "license": "MIT",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "keywords": [
    "components",
    "frontend",
    "library",
    "next",
    "nextjs",
    "react",
    "rich-text-editor",
    "rte",
    "tiptap",
    "ui",
    "ui-kit"
  ],
  "sideEffects": [
    "*.css"
  ],
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    },
    "./styles.css": "./esm/index.css",
    "./styles.layer.css": "./esm/index.layer.css"
  },
  "repository": {
    "url": "https://github.com/mantinedev/mantine.git",
    "type": "git",
    "directory": "src/mantine-tiptap"
  },
  "peerDependencies": {
    "@mantine/core": "7.3.0-alpha.0",
    "@mantine/hooks": "7.3.0-alpha.0",
    "@tabler/icons-react": ">=2.0.0",
    "@tiptap/extension-link": "^2.1.12",
    "@tiptap/react": "^2.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mantine/core": "workspace:*",
    "@mantine/hooks": "workspace:*",
    "@tests/core": "workspace:*"
  }
}
