{
  "name": "@mantine/notifications",
  "version": "7.8.0",
  "description": "Mantine notifications system",
  "homepage": "https://mantine.dev",
  "license": "MIT",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "keywords": [
    "components",
    "frontend",
    "library",
    "next",
