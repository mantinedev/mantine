    "directory": "packages/@mantine/notifications"
  },
  "peerDependencies": {
    "@mantine/core": "7.8.0",
    "@mantine/hooks": "7.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@mantine/store": "7.8.0",
    "react-transition-group": "4.4.5"
  },
  "devDependencies": {
    "@mantine-tests/core": "workspace:*",
