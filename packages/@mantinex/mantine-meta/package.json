{
  "name": "@mantinex/mantine-meta",
  "version": "1.0.2",
  "description": "Meta data for *.mantine.dev websites",
  "homepage": "https://mantine.dev/",
  "license": "MIT",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "keywords": [
    "react",
    "ui"
  ],
  "sideEffects": [
    "*.css"
  ],
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    }
  },
  "repository": {
    "url": "https://github.com/mantinedev/mantine.git",
    "type": "git",
    "directory": "packages/@mantinex/mantine-meta"
  }
}
